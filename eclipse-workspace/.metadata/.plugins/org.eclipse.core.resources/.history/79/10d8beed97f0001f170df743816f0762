package midterm;

public class Tester {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(angleToInterval(0));
		System.out.println(angleToInterval(180));
		System.out.println(angleToInterval(90));
	}
	public static double angleToInterval(double angle){
	    return angle/90.0 - 1;
	}
}
